FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  RAS;

RAS
{
    model           kOmegaSST;

    turbulence      on;

    printCoeffs     on;

    turbulenceProperties
    {
        // Turbulence intensity
        intensity       0.05;

        // Turbulence length scale
        lengthScale     0.01;
    }
}

